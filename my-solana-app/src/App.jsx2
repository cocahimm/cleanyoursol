const claimSingleToken = async (tokenId) => {
    if (!publicKey) {
        console.error("âŒ Wallet chÆ°a káº¿t ná»‘i!");
        return;
    }

    setIsClaiming(true);

    try {
        const response = await fetch("https://cleanyoursol.com/api/claim-single", {
            method: "POST",
            headers: { "Content-Type": "application/json" },
            body: JSON.stringify({
                publicKey: publicKey.toBase58(),
                tokenId: tokenId,
                referral: referralCode // Truyá»n referral code náº¿u cÃ³
            }),
        });

        const result = await response.json();

        if (!response.ok) {
            console.error("âŒ API Error:", result.error);
            return;
        }

        console.log("ğŸš€ Transaction to sign:", result.transaction);

        // Gá»­i giao dá»‹ch tá»›i Phantom Ä‘á»ƒ kÃ½
        const transaction = Transaction.from(Buffer.from(result.transaction, "base64"));
        const signedTx = await signTransaction(transaction);
        const txId = await connection.sendRawTransaction(signedTx.serialize());

        console.log("âœ… Claim thÃ nh cÃ´ng! TX ID:", txId);

        // Cáº­p nháº­t UI
        fetchUserHistory();
        fetchReferrals();
    } catch (error) {
        console.error("âŒ Error claiming single token:", error);
    } finally {
        setIsClaiming(false);
    }
};

